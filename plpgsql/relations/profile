create table public."PROFILE" (
  id uuid not null default gen_random_uuid (),
  user_id uuid not null default auth.uid (),
  hometown text null,
  short_bio text null,
  city text null,
  profile_pic text null,
  about_you text null,
  relationship text null,
  social_link text null,
  quote text null,
  life_event jsonb null default '[]'::jsonb,
  religion text null,
  political_views text null,
  language text null,
  workplace text null,
  professional text null,
  high_school text null,
  university text null,
  college text null,
  cover_pic text null,
  constraint PROFILE_pkey primary key (id)
) TABLESPACE pg_default;
